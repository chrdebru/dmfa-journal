    <!-- http://kg.socialsecurity.be/ont/dmfa#HalfTimeCareerInterruptionNotion -->


    <owl:DatatypeProperty rdf:about="http://kg.socialsecurity.be/ont/dmfa#HalfTimeCareerInterruptionNotion">
