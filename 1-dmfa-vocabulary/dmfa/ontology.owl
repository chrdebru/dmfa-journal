        <rdfs:isDefinedBy xml:lang="nl">De waarde 0 is niet meer toegelaten.</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="fr">La valeur 0 n&apos;est plus autorisée.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">I NS S</rdfs:label>
        <rdfs:label xml:lang="nl">IDENTIFICATIENUMMER BIJ DE SOCIALE ZEKERHEID - INSZ</rdfs:label>
        <rdfs:label xml:lang="fr">NUMÉRO D&apos;IDENTIFICATION DE LA SÉCURITÉ SOCIALE - NISS</rdfs:label>
