    <!-- http://kg.socialsecurity.be/ont/dmfa#NaturalPersonSequenceNbr -->


    <owl:DatatypeProperty rdf:about="http://kg.socialsecurity.be/ont/dmfa#NaturalPersonSequenceNbr">
        <rdfs:domain rdf:resource="http://kg.socialsecurity.be/ont/dmfa#NaturalPerson"/>
        <length rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</length>
        <presence xml:lang="fr">Indispensable</presence>
        <presence xml:lang="nl">Onmisbaar</presence>
        <type xml:lang="nl">Numeriek</type>
        <type xml:lang="fr">Numérique</type>
        <terms:identifier>00023</terms:identifier>
        <rdfs:comment xml:lang="fr">Numéro de suite des personnes physiques au sein d&apos;une déclaration. Ce numéro doit être unique au sein d&apos;une déclaration.</rdfs:comment>
        <rdfs:comment xml:lang="nl">Volgnummer van de natuurlijke personen binnen een aangifte. Dit nummer moet uniek zijn binnen een aangifte.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="fr">Nombre entier positif et élément de [1;9999999].</rdfs:isDefinedBy>
        <rdfs:isDefinedBy xml:lang="nl">Positief geheel getal en element van [1;9999999].</rdfs:isDefinedBy>
        <rdfs:label xml:lang="fr">NUMÉRO DE SUITE PERSONNE PHYSIQUE</rdfs:label>
        <rdfs:label xml:lang="en">Natural Person Sequence Nbr</rdfs:label>
        <rdfs:label xml:lang="nl">VOLGNUMMER NATUURLIJKE PERSOON</rdfs:label>
    </owl:DatatypeProperty>
    


